<Window x:Class="Ahed_project.Windows.GeometryTemplateWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Ahed_project.Windows"
        mc:Ignorable="d"
        Title="GeometryTemplateWindow" Height="450" Width="800">
    <Grid>
        <Image Source="{Binding Geometry.image_geometry, UpdateSourceTrigger=PropertyChanged}"  Grid.Column="0" Grid.Row="0">
        </Image>
    </Grid>
</Window>
