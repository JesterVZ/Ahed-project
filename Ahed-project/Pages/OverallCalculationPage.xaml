<Page x:Class="Ahed_project.Pages.OverallCalculationPage"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:Ahed_project.Pages"
      xmlns:custom ="clr-namespace:Ahed_project.UserControl" xmlns:custom1="clr-namespace:Ahed_project.UserControlsCustom"
      mc:Ignorable="d" 
      xmlns:converter="clr-namespace:Ahed_project.MasterData"
      d:DesignHeight="1600" d:DesignWidth="1800"
      Title="OverallCalculationPage">
    <Page.Resources>
        <ResourceDictionary>
            <converter:Converter x:Key="Converter"></converter:Converter>
        </ResourceDictionary>

    </Page.Resources>

    <Grid>
        <ScrollViewer HorizontalScrollBarVisibility="Visible" VerticalScrollBarVisibility="Visible">
            <Grid Background="White" Width="1800" HorizontalAlignment="Left">
                <StackPanel Orientation="Vertical">
                    <StackPanel Orientation="Horizontal">
                        <Button Content="Calculate" Command="{Binding Calculate}" Margin="0 0 12 0" HorizontalAlignment="Left" VerticalAlignment="Top" Width="121" Background="#fc6"/>
                        <Button IsEnabled="False" Content="Batch calculation"  HorizontalAlignment="Left" VerticalAlignment="Top" Width="121" Background="#fc6"/>
                    </StackPanel>
                    <StackPanel Orientation="Horizontal">
                        <Label Width="800" FontWeight="Bold" Grid.Column="0" Grid.Row="0">Flow Data</Label>
                        <Label Width="648" FontWeight="Bold" Grid.Column="0" Grid.Row="0">Pressure Drop</Label>
                        <CheckBox IsChecked="{Binding use_viscosity_correction, Mode=TwoWay, UpdateSourceTrigger=PropertyChanged}" Content="Use Viscosity Correction"/>
                    </StackPanel>
                    <StackPanel Orientation="Horizontal" Margin="0 0 0 40">
                        <Grid Width="800" Height="{Binding GridHeight, UpdateSourceTrigger=PropertyChanged}" VerticalAlignment="Top">
                            <Grid.ColumnDefinitions>
                                <ColumnDefinition Width="198*"/>
                                <ColumnDefinition Width="120*"/>
                                <ColumnDefinition Width="120*"/>
                                <ColumnDefinition Width="30*"/>
                                <ColumnDefinition Width="120*"/>
                                <ColumnDefinition Width="120*"/>
                            </Grid.ColumnDefinitions>
                            <Grid.RowDefinitions>
                                <RowDefinition/>
                                <RowDefinition/>
                                <RowDefinition/>
                                <RowDefinition/>
                                <RowDefinition/>
                                <RowDefinition/>
                                <RowDefinition/>
                                <RowDefinition/>
                                <RowDefinition/>
                                <RowDefinition Height="{Binding ScrapingFrequencyRow, UpdateSourceTrigger=PropertyChanged}"/>
                                <RowDefinition Height="{Binding MaximumViscosityRow, UpdateSourceTrigger=PropertyChanged}"/>
                                <RowDefinition/>
                                <RowDefinition/>
                                <RowDefinition/>
                                <RowDefinition/>
                                <RowDefinition/>
                                <RowDefinition/>
                                <RowDefinition/>
                                <RowDefinition/>
                            </Grid.RowDefinitions>
                            <Label Grid.Column="1" Grid.Row="0" Grid.ColumnSpan="2" HorizontalAlignment="Center" FontWeight="Bold">Tube Side</Label>
                            <Label Grid.Column="4" Grid.Row="0" Grid.ColumnSpan="2" HorizontalAlignment="Center" FontWeight="Bold">Shell Side</Label>
                            <TextBox Style="{StaticResource Colored}" Text="{Binding Overall.fluid_name_tube, Mode=TwoWay, UpdateSourceTrigger=PropertyChanged}" Grid.Column="1" Grid.Row="1" Grid.ColumnSpan="2"/>
                            <TextBox Style="{StaticResource Colored}" Text="{Binding Overall.fluid_name_shell, Mode=TwoWay, UpdateSourceTrigger=PropertyChanged}" Grid.Column="4" Grid.Row="1" Grid.ColumnSpan="2"/>

                            <Label Grid.Column="0" Grid.Row="1">Fluid name</Label>

                            <Label Grid.Column="1" Grid.Row="2" HorizontalContentAlignment="Center">Inlet</Label>
                            <Label Grid.Column="2" Grid.Row="2" HorizontalContentAlignment="Center">Outlet</Label>
                            <Label Grid.Column="4" Grid.Row="2" HorizontalContentAlignment="Center">Inlet</Label>
                            <Label Grid.Column="5" Grid.Row="2" HorizontalContentAlignment="Center">Outlet</Label>

                            <Label Grid.Column="0" Grid.Row="3">Flow (kg/hr)</Label>
                            <TextBox  x:Name="flow_tube" PreviewMouseLeftButtonDown="ShowOnClick" MouseLeave="MouseLeave" Style="{StaticResource Default}" Text="{Binding Overall.flow_tube, Converter={StaticResource Converter}, Mode=TwoWay, UpdateSourceTrigger=PropertyChanged}" Grid.Row="3" Grid.Column="1" Grid.ColumnSpan="2"/>
                            <TextBox  x:Name="flow_shell" PreviewMouseLeftButtonDown="ShowOnClick" MouseLeave="MouseLeave" Style="{StaticResource Default}" Text="{Binding Overall.flow_shell, Converter={StaticResource Converter}, Mode=TwoWay, UpdateSourceTrigger=PropertyChanged}" Grid.Row="3" Grid.Column="4" Grid.ColumnSpan="2"/>

                            <Label Grid.Column="0" Grid.Row="4">Temperature (°C)</Label>
                            <TextBox  x:Name="temperature_tube_inlet" PreviewMouseLeftButtonDown="ShowOnClick" MouseLeave="MouseLeave" Style="{StaticResource Default}" Text="{Binding Overall.temperature_tube_inlet, Converter={StaticResource Converter}, Mode=TwoWay, UpdateSourceTrigger=PropertyChanged}" Grid.Row="4" Grid.Column="1"/>
                            <TextBox  x:Name="temperature_tube_outlet" PreviewMouseLeftButtonDown="ShowOnClick" MouseLeave="MouseLeave" Style="{StaticResource Default}" Text="{Binding Overall.temperature_tube_outlet, Converter={StaticResource Converter}, Mode=TwoWay, UpdateSourceTrigger=PropertyChanged}" Grid.Row="4" Grid.Column="2"/>

                            <TextBox  x:Name="temperature_shell_inlet" PreviewMouseLeftButtonDown="ShowOnClick" MouseLeave="MouseLeave" Style="{StaticResource Default}" Text="{Binding Overall.temperature_shell_inlet, Converter={StaticResource Converter}, Mode=TwoWay, UpdateSourceTrigger=PropertyChanged}" Grid.Row="4" Grid.Column="4"/>
                            <TextBox  x:Name="temperature_shell_outlet" PreviewMouseLeftButtonDown="ShowOnClick" MouseLeave="MouseLeave" Style="{StaticResource Colored}" Text="{Binding Overall.temperature_shell_outlet, Converter={StaticResource Converter}, Mode=TwoWay, UpdateSourceTrigger=PropertyChanged}" Grid.Row="4" Grid.Column="5"/>

                            <Label Grid.Column="0" Grid.Row="5">Duty (kW)</Label>
                            <TextBox  x:Name="duty_tube" PreviewMouseLeftButtonDown="ShowOnClick" MouseLeave="MouseLeave" Style="{StaticResource Colored}" Text="{Binding Overall.duty_tube, Mode=TwoWay, Converter={StaticResource Converter}, UpdateSourceTrigger=PropertyChanged}" Grid.Row="5" Grid.Column="1" Grid.ColumnSpan="2"/>
                            <TextBox  x:Name="duty_shell" PreviewMouseLeftButtonDown="ShowOnClick" MouseLeave="MouseLeave" Style="{StaticResource Colored}" Text="{Binding Overall.duty_shell, Mode=TwoWay, Converter={StaticResource Converter}, UpdateSourceTrigger=PropertyChanged}" Grid.Row="5" Grid.Column="4" Grid.ColumnSpan="2"/>

                            <Label Grid.Column="0" Grid.Row="6">Fluid Velocity (m/s)</Label>
                            <TextBox  x:Name="fluid_velocity_tube_inlet" PreviewMouseLeftButtonDown="ShowOnClick" MouseLeave="MouseLeave" Style="{StaticResource Colored}" Text="{Binding Overall.fluid_velocity_tube_inlet, Converter={StaticResource Converter}, Mode=TwoWay, UpdateSourceTrigger=PropertyChanged}" Grid.Row="6" Grid.Column="1"/>
                            <TextBox  x:Name="fluid_velocity_tube_outlet" PreviewMouseLeftButtonDown="ShowOnClick" MouseLeave="MouseLeave" Style="{StaticResource Colored}" Text="{Binding Overall.fluid_velocity_tube_outlet, Converter={StaticResource Converter}, Mode=TwoWay, UpdateSourceTrigger=PropertyChanged}" Grid.Row="6" Grid.Column="2"/>

                            <TextBox  x:Name="fluid_velocity_shell_inlet" PreviewMouseLeftButtonDown="ShowOnClick" MouseLeave="MouseLeave" Style="{StaticResource Colored}" Text="{Binding Overall.fluid_velocity_shell_inlet, Converter={StaticResource Converter}, Mode=TwoWay, UpdateSourceTrigger=PropertyChanged}" Grid.Row="6" Grid.Column="4"/>
                            <TextBox  x:Name="fluid_velocity_shell_outlet" PreviewMouseLeftButtonDown="ShowOnClick" MouseLeave="MouseLeave" Style="{StaticResource Colored}" Text="{Binding Overall.fluid_velocity_shell_outlet, Converter={StaticResource Converter}, Mode=TwoWay, UpdateSourceTrigger=PropertyChanged}" Grid.Row="6" Grid.Column="5"/>

                            <Label Grid.Column="0" Grid.Row="7">Shear rate (1/S)</Label>
                            <TextBox  x:Name="shear_rate_tube_inlet" PreviewMouseLeftButtonDown="ShowOnClick" MouseLeave="MouseLeave" Style="{StaticResource Colored}" Text="{Binding Overall.shear_rate_tube_inlet, Converter={StaticResource Converter}, Mode=TwoWay, UpdateSourceTrigger=PropertyChanged}" Grid.Row="7" Grid.Column="1"/>
                            <TextBox  x:Name="shear_rate_tube_outlet" PreviewMouseLeftButtonDown="ShowOnClick" MouseLeave="MouseLeave" Style="{StaticResource Colored}" Text="{Binding Overall.shear_rate_tube_outlet, Converter={StaticResource Converter}, Mode=TwoWay, UpdateSourceTrigger=PropertyChanged}" Grid.Row="7" Grid.Column="2"/>

                            <TextBox  x:Name="shear_rate_shell_inlet" PreviewMouseLeftButtonDown="ShowOnClick" MouseLeave="MouseLeave" Style="{StaticResource Colored}" Text="{Binding Overall.shear_rate_shell_inlet, Converter={StaticResource Converter}, Mode=TwoWay, UpdateSourceTrigger=PropertyChanged}" Grid.Row="7" Grid.Column="4"/>
                            <TextBox  x:Name="shear_rate_shell_outlet" PreviewMouseLeftButtonDown="ShowOnClick" MouseLeave="MouseLeave" Style="{StaticResource Colored}" Text="{Binding Overall.shear_rate_shell_outlet, Converter={StaticResource Converter}, Mode=TwoWay, UpdateSourceTrigger=PropertyChanged}" Grid.Row="7" Grid.Column="5"/>

                            <Label Grid.Column="0" Grid.Row="8">Flow Type</Label>
                            <TextBox x:Name="flow_type_tube_inlet" PreviewMouseLeftButtonDown="ShowOnClick" MouseLeave="MouseLeave" Style="{StaticResource Colored}" Text="{Binding Overall.flow_type_tube_inlet, Converter={StaticResource Converter}, Mode=TwoWay, UpdateSourceTrigger=PropertyChanged}" Grid.Row="8" Grid.Column="1"/>
                            <TextBox x:Name="flow_type_tube_outlet" PreviewMouseLeftButtonDown="ShowOnClick" MouseLeave="MouseLeave" Style="{StaticResource Colored}" Text="{Binding Overall.flow_type_tube_outlet, Converter={StaticResource Converter}, Mode=TwoWay, UpdateSourceTrigger=PropertyChanged}" Grid.Row="8" Grid.Column="2"/>

                            <TextBox x:Name="flow_type_shell_inlet" PreviewMouseLeftButtonDown="ShowOnClick" MouseLeave="MouseLeave" Style="{StaticResource Colored}" Text="{Binding Overall.flow_type_shell_inlet, Converter={StaticResource Converter}, Mode=TwoWay, UpdateSourceTrigger=PropertyChanged}" Grid.Row="8" Grid.Column="4"/>
                            <TextBox x:Name="flow_type_shell_outlet" PreviewMouseLeftButtonDown="ShowOnClick" MouseLeave="MouseLeave" Style="{StaticResource Colored}" Text="{Binding Overall.flow_type_shell_outlet, Converter={StaticResource Converter}, Mode=TwoWay, UpdateSourceTrigger=PropertyChanged}" Grid.Row="8" Grid.Column="5"/>

                            <Label Grid.Column="0" Grid.Row="9">Scraping Frequency</Label>
                            <TextBox Style="{StaticResource Default}" Grid.Row="9" Grid.Column="1" Grid.ColumnSpan="2"/>

                            <Label Grid.Column="0" Grid.Row="10">Maximum Visosity</Label>
                            <TextBox Style="{StaticResource Default}" Grid.Row="10" Grid.Column="1" Grid.ColumnSpan="2"/>

                            <Label Grid.Column="0" Grid.Row="11" FontWeight="Bold">Liquid Phase</Label>

                            <Label Grid.Column="0" Grid.Row="12">App. Viscosity (cP)</Label>
                            <TextBox x:Name="liquid_phase_app_viscosity_tube_inlet" PreviewMouseLeftButtonDown="ShowOnClick" MouseLeave="MouseLeave" Style="{StaticResource Colored}" Text="{Binding Overall.liquid_phase_app_viscosity_tube_inlet, Converter={StaticResource Converter}, Mode=TwoWay, UpdateSourceTrigger=PropertyChanged}" Grid.Row="12" Grid.Column="1"/>
                            <TextBox x:Name="liquid_phase_app_viscosity_tube_outlet" PreviewMouseLeftButtonDown="ShowOnClick" MouseLeave="MouseLeave" Style="{StaticResource Colored}" Text="{Binding Overall.liquid_phase_app_viscosity_tube_outlet, Converter={StaticResource Converter}, Mode=TwoWay, UpdateSourceTrigger=PropertyChanged}" Grid.Row="12" Grid.Column="2"/>

                            <TextBox x:Name="liquid_phase_app_viscosity_shell_inlet" PreviewMouseLeftButtonDown="ShowOnClick" MouseLeave="MouseLeave" Style="{StaticResource Colored}" Text="{Binding Overall.liquid_phase_app_viscosity_shell_inlet, Converter={StaticResource Converter}, Mode=TwoWay, UpdateSourceTrigger=PropertyChanged}" Grid.Row="12" Grid.Column="4"/>
                            <TextBox x:Name="liquid_phase_app_viscosity_shell_outlet" PreviewMouseLeftButtonDown="ShowOnClick" MouseLeave="MouseLeave" Style="{StaticResource Colored}" Text="{Binding Overall.liquid_phase_app_viscosity_shell_outlet, Converter={StaticResource Converter}, Mode=TwoWay, UpdateSourceTrigger=PropertyChanged}" Grid.Row="12" Grid.Column="5"/>

                            <Label Grid.Column="0" Grid.Row="13">Reynolds</Label>
                            <TextBox x:Name="liquid_phase_reynolds_tube_inlet" PreviewMouseLeftButtonDown="ShowOnClick" MouseLeave="MouseLeave" Style="{StaticResource Colored}" Text="{Binding Overall.liquid_phase_reynolds_tube_inlet, Converter={StaticResource Converter}, Mode=TwoWay, UpdateSourceTrigger=PropertyChanged}" Grid.Row="13" Grid.Column="1"/>
                            <TextBox x:Name="liquid_phase_reynolds_tube_outlet" PreviewMouseLeftButtonDown="ShowOnClick" MouseLeave="MouseLeave" Style="{StaticResource Colored}" Text="{Binding Overall.liquid_phase_reynolds_tube_outlet, Converter={StaticResource Converter}, Mode=TwoWay, UpdateSourceTrigger=PropertyChanged}" Grid.Row="13" Grid.Column="2"/>

                            <TextBox x:Name="liquid_phase_reynolds_shell_inlet" PreviewMouseLeftButtonDown="ShowOnClick" MouseLeave="MouseLeave" Style="{StaticResource Colored}" Text="{Binding Overall.liquid_phase_reynolds_shell_inlet, Converter={StaticResource Converter}, Mode=TwoWay, UpdateSourceTrigger=PropertyChanged}" Grid.Row="13" Grid.Column="4"/>
                            <TextBox x:Name="liquid_phase_reynolds_shell_outlet" PreviewMouseLeftButtonDown="ShowOnClick" MouseLeave="MouseLeave" Style="{StaticResource Colored}" Text="{Binding Overall.liquid_phase_reynolds_shell_outlet, Converter={StaticResource Converter}, Mode=TwoWay, UpdateSourceTrigger=PropertyChanged}" Grid.Row="13" Grid.Column="5"/>

                            <Label Grid.Column="0" Grid.Row="14">Prandtl</Label>
                            <TextBox x:Name="liquid_phase_prandtl_tube_inlet" PreviewMouseLeftButtonDown="ShowOnClick" MouseLeave="MouseLeave" Style="{StaticResource Colored}" Text="{Binding Overall.liquid_phase_prandtl_tube_inlet, Converter={StaticResource Converter}, Mode=TwoWay, UpdateSourceTrigger=PropertyChanged}" Grid.Row="14" Grid.Column="1"/>
                            <TextBox x:Name="liquid_phase_prandtl_tube_outlet" PreviewMouseLeftButtonDown="ShowOnClick" MouseLeave="MouseLeave" Style="{StaticResource Colored}" Text="{Binding Overall.liquid_phase_prandtl_tube_outlet, Converter={StaticResource Converter}, Mode=TwoWay, UpdateSourceTrigger=PropertyChanged}" Grid.Row="14" Grid.Column="2"/>

                            <TextBox x:Name="liquid_phase_prandtl_shell_inlet" PreviewMouseLeftButtonDown="ShowOnClick" MouseLeave="MouseLeave" Style="{StaticResource Colored}" Text="{Binding Overall.liquid_phase_prandtl_shell_inlet, Converter={StaticResource Converter}, Mode=TwoWay, UpdateSourceTrigger=PropertyChanged}" Grid.Row="14" Grid.Column="4"/>
                            <TextBox x:Name="liquid_phase_prandtl_shell_outlet" PreviewMouseLeftButtonDown="ShowOnClick" MouseLeave="MouseLeave" Style="{StaticResource Colored}" Text="{Binding Overall.liquid_phase_prandtl_shell_outlet, Converter={StaticResource Converter}, Mode=TwoWay, UpdateSourceTrigger=PropertyChanged}" Grid.Row="14" Grid.Column="5"/>

                            <Label Grid.Column="0" Grid.Row="15" FontWeight="Bold">Gas Phase</Label>

                            <Label Grid.Column="0" Grid.Row="16">Dynamic viscosity gas (cP)</Label>
                            <TextBox x:Name="gas_phase_app_viscosity_tube_inlet" PreviewMouseLeftButtonDown="ShowOnClick" MouseLeave="MouseLeave" Style="{StaticResource Colored}" Text="{Binding Overall.gas_phase_app_viscosity_tube_inlet, Converter={StaticResource Converter}, Mode=TwoWay, UpdateSourceTrigger=PropertyChanged}" Grid.Row="16" Grid.Column="1"/>
                            <TextBox x:Name="gas_phase_app_viscosity_tube_outlet" PreviewMouseLeftButtonDown="ShowOnClick" MouseLeave="MouseLeave" Style="{StaticResource Colored}" Text="{Binding Overall.gas_phase_app_viscosity_tube_outlet, Converter={StaticResource Converter}, Mode=TwoWay, UpdateSourceTrigger=PropertyChanged}" Grid.Row="16" Grid.Column="2"/>

                            <TextBox x:Name="gas_phase_app_viscosity_shell_inlet" PreviewMouseLeftButtonDown="ShowOnClick" MouseLeave="MouseLeave" Style="{StaticResource Colored}" Text="{Binding Overall.gas_phase_app_viscosity_shell_inlet, Converter={StaticResource Converter}, Mode=TwoWay, UpdateSourceTrigger=PropertyChanged}" Grid.Row="16" Grid.Column="4"/>
                            <TextBox x:Name="gas_phase_app_viscosity_shell_outlet" PreviewMouseLeftButtonDown="ShowOnClick" MouseLeave="MouseLeave" Style="{StaticResource Colored}" Text="{Binding Overall.gas_phase_app_viscosity_shell_outlet, Converter={StaticResource Converter}, Mode=TwoWay, UpdateSourceTrigger=PropertyChanged}" Grid.Row="16" Grid.Column="5"/>

                            <Label Grid.Column="0" Grid.Row="17">Reynolds</Label>
                            <TextBox x:Name="gas_phase_reynolds_tube_inlet" PreviewMouseLeftButtonDown="ShowOnClick" MouseLeave="MouseLeave" Style="{StaticResource Colored}" Text="{Binding Overall.gas_phase_reynolds_tube_inlet, Converter={StaticResource Converter}, Mode=TwoWay, UpdateSourceTrigger=PropertyChanged}" Grid.Row="17" Grid.Column="1"/>
                            <TextBox x:Name="gas_phase_reynolds_tube_outlet" PreviewMouseLeftButtonDown="ShowOnClick" MouseLeave="MouseLeave" Style="{StaticResource Colored}" Text="{Binding Overall.gas_phase_reynolds_tube_outlet, Converter={StaticResource Converter}, Mode=TwoWay, UpdateSourceTrigger=PropertyChanged}" Grid.Row="17" Grid.Column="2"/>

                            <TextBox x:Name="gas_phase_reynolds_shell_inlet" PreviewMouseLeftButtonDown="ShowOnClick" MouseLeave="MouseLeave" Style="{StaticResource Colored}" Text="{Binding Overall.gas_phase_reynolds_shell_inlet, Converter={StaticResource Converter}, Mode=TwoWay, UpdateSourceTrigger=PropertyChanged}" Grid.Row="17" Grid.Column="4"/>
                            <TextBox x:Name="gas_phase_reynolds_shell_outlet" PreviewMouseLeftButtonDown="ShowOnClick" MouseLeave="MouseLeave" Style="{StaticResource Colored}" Text="{Binding Overall.gas_phase_reynolds_shell_outlet, Converter={StaticResource Converter}, Mode=TwoWay, UpdateSourceTrigger=PropertyChanged}" Grid.Row="17" Grid.Column="5"/>

                            <Label Grid.Column="0" Grid.Row="18">Prandtl</Label>
                            <TextBox x:Name="gas_phase_prandtl_tube_inlet" PreviewMouseLeftButtonDown="ShowOnClick" MouseLeave="MouseLeave" Style="{StaticResource Colored}" Text="{Binding Overall.gas_phase_prandtl_tube_inlet, Converter={StaticResource Converter}, Mode=TwoWay, UpdateSourceTrigger=PropertyChanged}" Grid.Row="18" Grid.Column="1"/>
                            <TextBox x:Name="gas_phase_prandtl_tube_outlet" PreviewMouseLeftButtonDown="ShowOnClick" MouseLeave="MouseLeave" Style="{StaticResource Colored}" Text="{Binding Overall.gas_phase_prandtl_tube_outlet, Converter={StaticResource Converter}, Mode=TwoWay, UpdateSourceTrigger=PropertyChanged}" Grid.Row="18" Grid.Column="2"/>

                            <TextBox x:Name="gas_phase_prandtl_shell_inlet" PreviewMouseLeftButtonDown="ShowOnClick" MouseLeave="MouseLeave" Style="{StaticResource Colored}" Text="{Binding Overall.gas_phase_prandtl_shell_inlet, Converter={StaticResource Converter}, Mode=TwoWay, UpdateSourceTrigger=PropertyChanged}" Grid.Row="18" Grid.Column="4"/>
                            <TextBox x:Name="gas_phase_prandtl_shell_outlet" PreviewMouseLeftButtonDown="ShowOnClick" MouseLeave="MouseLeave" Style="{StaticResource Colored}" Text="{Binding Overall.gas_phase_prandtl_shell_outlet, Converter={StaticResource Converter}, Mode=TwoWay, UpdateSourceTrigger=PropertyChanged}" Grid.Row="18" Grid.Column="5"/>

                        </Grid>
                        <Grid Width="800" Height="745" VerticalAlignment="Top">
                            <Grid.ColumnDefinitions>
                                <ColumnDefinition Width="198*"/>
                                <ColumnDefinition Width="120*"/>
                                <ColumnDefinition Width="120*"/>
                                <ColumnDefinition Width="120*"/>
                            </Grid.ColumnDefinitions>
                            <Grid.RowDefinitions>
                                <RowDefinition/>
                                <RowDefinition/>
                                <RowDefinition/>
                                <RowDefinition/>
                                <RowDefinition/>
                                <RowDefinition/>
                                <RowDefinition/>
                                <RowDefinition/>
                                <RowDefinition/>
                                <RowDefinition/>
                                <RowDefinition/>
                                <RowDefinition/>
                                <RowDefinition/>
                                <RowDefinition/>
                                <RowDefinition/>
                                <RowDefinition/>
                                <RowDefinition/>
                                <RowDefinition/>
                                <RowDefinition/>
                            </Grid.RowDefinitions>
                            <Label Grid.Column="0" Grid.Row="0" FontWeight="Bold">Tubes Side</Label>
                            <Label Grid.Column="1" Grid.Row="1" HorizontalContentAlignment="Center" VerticalContentAlignment="Center">V</Label>
                            <Label Grid.Column="1" Grid.Row="1" HorizontalContentAlignment="Right" VerticalContentAlignment="Center">m/s</Label>

                            <TextBlock Grid.Column="2" Grid.Row="1" HorizontalAlignment="Center">P·V<Run BaselineAlignment="Superscript">2</Run></TextBlock>
                            <TextBlock Grid.Column="2" Grid.Row="1" HorizontalAlignment="Right" >kg/m*s<Run BaselineAlignment="Superscript">2</Run></TextBlock>

                            <TextBlock Grid.Column="3" Grid.Row="1" HorizontalAlignment="Center" VerticalAlignment="Center">ΔP</TextBlock>
                            <TextBlock Grid.Column="3" Grid.Row="1" HorizontalAlignment="Right" VerticalAlignment="Center">bar-a</TextBlock>

                            <Label Grid.Column="0" Grid.Row="2">Modules</Label>
                            <TextBox x:Name="pressure_drop_tube_side_modules_V" PreviewMouseLeftButtonDown="ShowOnClick" MouseLeave="MouseLeave" Grid.Column="1" Grid.Row="2" Style="{StaticResource Colored}" Text="{Binding Overall.pressure_drop_tube_side_modules_V, Converter={StaticResource Converter}, Mode=TwoWay, UpdateSourceTrigger=PropertyChanged}"/>
                            <TextBox x:Name="pressure_drop_tube_side_modules_P" PreviewMouseLeftButtonDown="ShowOnClick" MouseLeave="MouseLeave" Grid.Column="3" Grid.Row="2" Style="{StaticResource Colored}" Text="{Binding Overall.pressure_drop_tube_side_modules_P, Converter={StaticResource Converter}, Mode=TwoWay, UpdateSourceTrigger=PropertyChanged}"/>

                            <Label Grid.Column="0" Grid.Row="3">Inlet Nozzles</Label>
                            <TextBox x:Name="pressure_drop_tube_side_inlet_nozzles_V" PreviewMouseLeftButtonDown="ShowOnClick" MouseLeave="MouseLeave" Grid.Column="1" Grid.Row="3" Style="{StaticResource Colored}" Text="{Binding Overall.pressure_drop_tube_side_inlet_nozzles_V, Converter={StaticResource Converter}, Mode=TwoWay, UpdateSourceTrigger=PropertyChanged}"/>
                            <TextBox x:Name="pressure_drop_tube_side_inlet_nozzles_pV" PreviewMouseLeftButtonDown="ShowOnClick" MouseLeave="MouseLeave" Grid.Column="2" Grid.Row="3" Style="{StaticResource Colored}" Text="{Binding Overall.pressure_drop_tube_side_inlet_nozzles_pV, Converter={StaticResource Converter}, Mode=TwoWay, UpdateSourceTrigger=PropertyChanged}"/>
                            <TextBox x:Name="pressure_drop_tube_side_inlet_nozzles_P" PreviewMouseLeftButtonDown="ShowOnClick" MouseLeave="MouseLeave" Grid.Column="3" Grid.Row="3" Style="{StaticResource Colored}" Text="{Binding Overall.pressure_drop_tube_side_inlet_nozzles_P, Converter={StaticResource Converter}, Mode=TwoWay, UpdateSourceTrigger=PropertyChanged}"/>

                            <Label Grid.Column="0" Grid.Row="4">Outlet Nozzles</Label>
                            <TextBox x:Name="pressure_drop_tube_side_outlet_nozzles_V" PreviewMouseLeftButtonDown="ShowOnClick" MouseLeave="MouseLeave" Grid.Column="1" Grid.Row="4" Style="{StaticResource Colored}" Text="{Binding Overall.pressure_drop_tube_side_outlet_nozzles_V, Converter={StaticResource Converter}, Mode=TwoWay, UpdateSourceTrigger=PropertyChanged}"/>
                            <TextBox x:Name="pressure_drop_tube_side_outlet_nozzles_pV" PreviewMouseLeftButtonDown="ShowOnClick" MouseLeave="MouseLeave" Grid.Column="2" Grid.Row="4" Style="{StaticResource Colored}" Text="{Binding Overall.pressure_drop_tube_side_outlet_nozzles_pV, Converter={StaticResource Converter}, Mode=TwoWay, UpdateSourceTrigger=PropertyChanged}"/>
                            <TextBox x:Name="pressure_drop_tube_side_outlet_nozzles_P" PreviewMouseLeftButtonDown="ShowOnClick" MouseLeave="MouseLeave" Grid.Column="3" Grid.Row="4" Style="{StaticResource Colored}" Text="{Binding Overall.pressure_drop_tube_side_outlet_nozzles_P, Converter={StaticResource Converter}, Mode=TwoWay, UpdateSourceTrigger=PropertyChanged}"/>

                            <Label Grid.Column="0" Grid.Row="5">Bends</Label>
                            <TextBox x:Name="pressure_drop_tube_side_bends_V" PreviewMouseLeftButtonDown="ShowOnClick" MouseLeave="MouseLeave" Grid.Column="1" Grid.Row="5" Style="{StaticResource Colored}" Text="{Binding Overall.pressure_drop_tube_side_bends_V, Converter={StaticResource Converter}, Mode=TwoWay, UpdateSourceTrigger=PropertyChanged}"/>
                            <TextBox x:Name="pressure_drop_tube_side_bends_P" PreviewMouseLeftButtonDown="ShowOnClick" MouseLeave="MouseLeave" Grid.Column="3" Grid.Row="5" Style="{StaticResource Colored}" Text="{Binding Overall.pressure_drop_tube_side_bends_P, Converter={StaticResource Converter}, Mode=TwoWay, UpdateSourceTrigger=PropertyChanged}"/>

                            <Label Grid.Column="0" Grid.Row="6" FontWeight="Bold">Total</Label>
                            <TextBox x:Name="pressure_drop_tube_side_total_P" PreviewMouseLeftButtonDown="ShowOnClick" MouseLeave="MouseLeave" Grid.Column="3" Grid.Row="6" Style="{StaticResource Colored}" Text="{Binding Overall.pressure_drop_tube_side_total_P, Converter={StaticResource Converter}, Mode=TwoWay, UpdateSourceTrigger=PropertyChanged}"/>

                            <Label Grid.Column="0" Grid.Row="8" FontWeight="Bold">Shell Side</Label>

                            <Label Grid.Column="1" Grid.Row="8" HorizontalContentAlignment="Center" VerticalContentAlignment="Center">V</Label>
                            <Label Grid.Column="1" Grid.Row="8" HorizontalContentAlignment="Right" VerticalContentAlignment="Center">m/s</Label>

                            <TextBlock Grid.Column="2" Grid.Row="8" HorizontalAlignment="Center">P·V<Run BaselineAlignment="Superscript">2</Run></TextBlock>
                            <TextBlock Grid.Column="2" Grid.Row="8" HorizontalAlignment="Right" >kg/m*s<Run BaselineAlignment="Superscript">2</Run></TextBlock>

                            <TextBlock Grid.Column="3" Grid.Row="8" HorizontalAlignment="Center" VerticalAlignment="Center">ΔP</TextBlock>
                            <TextBlock Grid.Column="3" Grid.Row="8" HorizontalAlignment="Right" VerticalAlignment="Center">bar-a</TextBlock>

                            <Label Grid.Column="0" Grid.Row="9">Modules</Label>
                            <TextBox x:Name="pressure_drop_shell_side_modules_V" PreviewMouseLeftButtonDown="ShowOnClick" MouseLeave="MouseLeave" Grid.Column="1" Grid.Row="9" Style="{StaticResource Colored}" Text="{Binding Overall.pressure_drop_shell_side_modules_V, Converter={StaticResource Converter}, Mode=TwoWay, UpdateSourceTrigger=PropertyChanged}"/>
