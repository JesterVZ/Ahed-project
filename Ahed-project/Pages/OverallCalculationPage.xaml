                            <Label Grid.Column="0" Grid.Row="10">Vitrex Shedding Ratio</Label>
                            <TextBox x:Name="vibrations_inlet_vortex_shedding_ratio" PreviewMouseLeftButtonDown="ShowOnClick" MouseLeave="MouseLeave" Style="{StaticResource Colored}" Grid.Column="1" Grid.Row="10" Text="{Binding Overall.vibrations_inlet_vortex_shedding_ratio, Converter={StaticResource Converter}, Mode=TwoWay, UpdateSourceTrigger=PropertyChanged}"/>
                            <TextBox x:Name="vibrations_central_vortex_shedding_ratio" PreviewMouseLeftButtonDown="ShowOnClick" MouseLeave="MouseLeave" Style="{StaticResource Colored}" Grid.Column="2" Grid.Row="10" Text="{Binding Overall.vibrations_central_vortex_shedding_ratio, Converter={StaticResource Converter}, Mode=TwoWay, UpdateSourceTrigger=PropertyChanged}"/>
                            <TextBox x:Name="vibrations_outlet_vortex_shedding_ratio" PreviewMouseLeftButtonDown="ShowOnClick" MouseLeave="MouseLeave" Style="{StaticResource Colored}" Grid.Column="3" Grid.Row="10" Text="{Binding Overall.vibrations_outlet_vortex_shedding_ratio, Converter={StaticResource Converter}, Mode=TwoWay, UpdateSourceTrigger=PropertyChanged}"/>

                            <Label Grid.Column="0" Grid.Row="11">Turbulent Buffeting Ratio</Label>
                            <TextBox x:Name="vibrations_inlet_turbulent_buffeting_ratio" PreviewMouseLeftButtonDown="ShowOnClick" MouseLeave="MouseLeave" Style="{StaticResource Colored}" Grid.Column="1" Grid.Row="11" Text="{Binding Overall.vibrations_inlet_turbulent_buffeting_ratio, Converter={StaticResource Converter}, Mode=TwoWay, UpdateSourceTrigger=PropertyChanged}"/>
                            <TextBox x:Name="vibrations_central_turbulent_buffeting_ratio" PreviewMouseLeftButtonDown="ShowOnClick" MouseLeave="MouseLeave" Style="{StaticResource Colored}" Grid.Column="2" Grid.Row="11" Text="{Binding Overall.vibrations_central_turbulent_buffeting_ratio, Converter={StaticResource Converter}, Mode=TwoWay, UpdateSourceTrigger=PropertyChanged}"/>
                            <TextBox x:Name="vibrations_outlet_turbulent_buffeting_ratio" PreviewMouseLeftButtonDown="ShowOnClick" MouseLeave="MouseLeave" Style="{StaticResource Colored}" Grid.Column="3" Grid.Row="11" Text="{Binding Overall.vibrations_outlet_turbulent_buffeting_ratio, Converter={StaticResource Converter}, Mode=TwoWay, UpdateSourceTrigger=PropertyChanged}"/>

                            <Label Grid.Column="0" Grid.Row="12" FontWeight="Bold">Acoustic Vibrations (gases)</Label>
                            <Label Grid.Column="0" Grid.Row="13">Acoustic Vibrations Exists</Label>

                            <CheckBox Grid.Column="1" IsChecked="{Binding acoustic_vibration_exist_inlet, Mode=TwoWay, UpdateSourceTrigger=PropertyChanged}" Grid.Row="13" HorizontalAlignment="Center" VerticalAlignment="Center"/>
                            <CheckBox Grid.Column="2" IsChecked="{Binding acoustic_vibration_exist_central, Mode=TwoWay, UpdateSourceTrigger=PropertyChanged}" Grid.Row="13" HorizontalAlignment="Center" VerticalAlignment="Center"/>
                            <CheckBox Grid.Column="3" IsChecked="{Binding acoustic_vibration_exist_outlet, Mode=TwoWay, UpdateSourceTrigger=PropertyChanged}" Grid.Row="13" HorizontalAlignment="Center" VerticalAlignment="Center"/>

                            <Label Grid.Column="0" Grid.Row="14" FontWeight="Bold">Vibrations Exists</Label>
                            <CheckBox Grid.Column="1" Grid.Row="14" HorizontalAlignment="Center" VerticalAlignment="Center"/>
                            <Label Grid.Column="4" Grid.Row="2" Content="&#60; 1.0"></Label>
                            <Label Grid.Column="4" Grid.Row="8" Content="&#60; 0.8"></Label>
                            <Label Grid.Column="4" Grid.Row="10" Content="&#60; 1.0"></Label>
                            <Label Grid.Column="4" Grid.Row="11" Content="&#60; 1.0"></Label>
                        </Grid>
                       
                    </StackPanel>
                </StackPanel>

            </Grid>
        </ScrollViewer>
    </Grid>
</Page>
