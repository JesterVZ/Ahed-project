                            <TextBox Style="{StaticResource Colored}" Text="{Binding Overall.flow_type_shell_inlet, Mode=TwoWay, UpdateSourceTrigger=PropertyChanged}" Grid.Row="8" Grid.Column="4"/>
                            <TextBox Style="{StaticResource Colored}" Text="{Binding Overall.flow_type_shell_outlet, Mode=TwoWay, UpdateSourceTrigger=PropertyChanged}" Grid.Row="8" Grid.Column="5"/>

                            <Label Grid.Column="0" Grid.Row="9">Scraping Frequency</Label>
                            <TextBox Style="{StaticResource Default}" Grid.Row="9" Grid.Column="1" Grid.ColumnSpan="2"/>

                            <Label Grid.Column="0" Grid.Row="10">Maximum Visosity</Label>
                            <TextBox Style="{StaticResource Colored}" Grid.Row="10" Grid.Column="1" Grid.ColumnSpan="2"/>


                            <Label Grid.Column="0" Grid.Row="11" FontWeight="Bold">Liquid Phase</Label>

                            <Label Grid.Column="0" Grid.Row="12">App. Viscosity</Label>
                            <TextBox Style="{StaticResource Colored}" Text="{Binding Overall.liquid_phase_app_viscosity_tube_inlet, Mode=TwoWay, UpdateSourceTrigger=PropertyChanged}" Grid.Row="12" Grid.Column="1"/>
                            <TextBox Style="{StaticResource Colored}" Text="{Binding Overall.liquid_phase_app_viscosity_tube_outlet, Mode=TwoWay, UpdateSourceTrigger=PropertyChanged}" Grid.Row="12" Grid.Column="2"/>

                            <TextBox Style="{StaticResource Colored}" Text="{Binding Overall.liquid_phase_app_viscosity_shell_inlet, Mode=TwoWay, UpdateSourceTrigger=PropertyChanged}" Grid.Row="12" Grid.Column="4"/>
                            <TextBox Style="{StaticResource Colored}" Text="{Binding Overall.liquid_phase_app_viscosity_shell_outlet, Mode=TwoWay, UpdateSourceTrigger=PropertyChanged}" Grid.Row="12" Grid.Column="5"/>

                            <Label Grid.Column="0" Grid.Row="13">Reynolds</Label>
                            <TextBox Style="{StaticResource Colored}" Text="{Binding Overall.liquid_phase_reynolds_tube_inlet, Mode=TwoWay, UpdateSourceTrigger=PropertyChanged}" Grid.Row="13" Grid.Column="1"/>
                            <TextBox Style="{StaticResource Colored}" Text="{Binding Overall.liquid_phase_reynolds_tube_outlet, Mode=TwoWay, UpdateSourceTrigger=PropertyChanged}" Grid.Row="13" Grid.Column="2"/>

                            <TextBox Style="{StaticResource Colored}" Text="{Binding Overall.liquid_phase_reynolds_shell_inlet, Mode=TwoWay, UpdateSourceTrigger=PropertyChanged}" Grid.Row="13" Grid.Column="4"/>
                            <TextBox Style="{StaticResource Colored}" Text="{Binding Overall.liquid_phase_reynolds_shell_outlet, Mode=TwoWay, UpdateSourceTrigger=PropertyChanged}" Grid.Row="13" Grid.Column="5"/>

                            <Label Grid.Column="0" Grid.Row="14">Prandtl</Label>
                            <TextBox Style="{StaticResource Colored}" Text="{Binding Overall.liquid_phase_prandtl_tube_inlet, Mode=TwoWay, UpdateSourceTrigger=PropertyChanged}" Grid.Row="14" Grid.Column="1"/>
                            <TextBox Style="{StaticResource Colored}" Text="{Binding Overall.liquid_phase_prandtl_tube_outlet, Mode=TwoWay, UpdateSourceTrigger=PropertyChanged}" Grid.Row="14" Grid.Column="2"/>

                            <TextBox Style="{StaticResource Colored}" Text="{Binding Overall.liquid_phase_prandtl_shell_inlet, Mode=TwoWay, UpdateSourceTrigger=PropertyChanged}" Grid.Row="14" Grid.Column="4"/>
                            <TextBox Style="{StaticResource Colored}" Text="{Binding Overall.liquid_phase_prandtl_shell_outlet, Mode=TwoWay, UpdateSourceTrigger=PropertyChanged}" Grid.Row="14" Grid.Column="5"/>

                            <Label Grid.Column="0" Grid.Row="15" FontWeight="Bold">Gas Phase</Label>

                            <Label Grid.Column="0" Grid.Row="16">Dynamic viscosity gas</Label>
                            <TextBox Style="{StaticResource Colored}" Text="{Binding Overall.gas_phase_app_viscosity_tube_inlet, Mode=TwoWay, UpdateSourceTrigger=PropertyChanged}" Grid.Row="16" Grid.Column="1"/>
                            <TextBox Style="{StaticResource Colored}" Text="{Binding Overall.gas_phase_app_viscosity_tube_outlet, Mode=TwoWay, UpdateSourceTrigger=PropertyChanged}" Grid.Row="16" Grid.Column="2"/>

                            <TextBox Style="{StaticResource Colored}" Text="{Binding Overall.gas_phase_app_viscosity_shell_inlet, Mode=TwoWay, UpdateSourceTrigger=PropertyChanged}" Grid.Row="16" Grid.Column="4"/>
                            <TextBox Style="{StaticResource Colored}" Text="{Binding Overall.gas_phase_app_viscosity_shell_outlet, Mode=TwoWay, UpdateSourceTrigger=PropertyChanged}" Grid.Row="16" Grid.Column="5"/>

                            <Label Grid.Column="0" Grid.Row="17">Reynolds</Label>
                            <TextBox Style="{StaticResource Colored}" Text="{Binding Overall.gas_phase_reynolds_tube_inlet, Mode=TwoWay, UpdateSourceTrigger=PropertyChanged}" Grid.Row="17" Grid.Column="1"/>
                            <TextBox Style="{StaticResource Colored}" Text="{Binding Overall.gas_phase_reynolds_tube_outlet, Mode=TwoWay, UpdateSourceTrigger=PropertyChanged}" Grid.Row="17" Grid.Column="2"/>

                            <TextBox Style="{StaticResource Colored}" Text="{Binding Overall.gas_phase_reynolds_shell_inlet, Mode=TwoWay, UpdateSourceTrigger=PropertyChanged}" Grid.Row="17" Grid.Column="4"/>
                            <TextBox Style="{StaticResource Colored}" Text="{Binding Overall.gas_phase_reynolds_shell_outlet, Mode=TwoWay, UpdateSourceTrigger=PropertyChanged}" Grid.Row="17" Grid.Column="5"/>

                            <Label Grid.Column="0" Grid.Row="18">Prandtl</Label>
                            <TextBox Style="{StaticResource Colored}" Text="{Binding Overall.gas_phase_prandtl_tube_inlet, Mode=TwoWay, UpdateSourceTrigger=PropertyChanged}" Grid.Row="18" Grid.Column="1"/>
                            <TextBox Style="{StaticResource Colored}" Text="{Binding Overall.gas_phase_prandtl_tube_outlet, Mode=TwoWay, UpdateSourceTrigger=PropertyChanged}" Grid.Row="18" Grid.Column="2"/>

                            <TextBox Style="{StaticResource Colored}" Text="{Binding Overall.gas_phase_prandtl_shell_inlet, Mode=TwoWay, UpdateSourceTrigger=PropertyChanged}" Grid.Row="18" Grid.Column="4"/>
                            <TextBox Style="{StaticResource Colored}" Text="{Binding Overall.gas_phase_prandtl_shell_outlet, Mode=TwoWay, UpdateSourceTrigger=PropertyChanged}" Grid.Row="18" Grid.Column="5"/>

                        </Grid>
                        <Grid Width="800" Height="745" VerticalAlignment="Top">
                            <Grid.ColumnDefinitions>
                                <ColumnDefinition Width="198*"/>
                                <ColumnDefinition Width="120*"/>
                                <ColumnDefinition Width="120*"/>
                                <ColumnDefinition Width="120*"/>
                            </Grid.ColumnDefinitions>
                            <Grid.RowDefinitions>
                                <RowDefinition/>
                                <RowDefinition/>
                                <RowDefinition/>
                                <RowDefinition/>
                                <RowDefinition/>
                                <RowDefinition/>
                                <RowDefinition/>
                                <RowDefinition/>
                                <RowDefinition/>
                                <RowDefinition/>
                                <RowDefinition/>
                                <RowDefinition/>
                                <RowDefinition/>
                                <RowDefinition/>
                                <RowDefinition/>
                                <RowDefinition/>
                                <RowDefinition/>
                                <RowDefinition/>
                                <RowDefinition/>
                            </Grid.RowDefinitions>
                            <Label Grid.Column="0" Grid.Row="0" FontWeight="Bold">Tubes Side</Label>
                            <Label Grid.Column="1" Grid.Row="1" HorizontalContentAlignment="Center" VerticalContentAlignment="Center">V</Label>
                            <Label Grid.Column="1" Grid.Row="1" HorizontalContentAlignment="Right" VerticalContentAlignment="Center">m/s</Label>

                            <TextBlock Grid.Column="2" Grid.Row="1" HorizontalAlignment="Center">P*V<Run BaselineAlignment="Superscript">2</Run></TextBlock>
                            <TextBlock Grid.Column="2" Grid.Row="1" HorizontalAlignment="Right" >kg/m*s<Run BaselineAlignment="Superscript">2</Run></TextBlock>

                            <TextBlock Grid.Column="3" Grid.Row="1" HorizontalAlignment="Center" VerticalAlignment="Center">ΔP</TextBlock>
                            <TextBlock Grid.Column="3" Grid.Row="1" HorizontalAlignment="Right" VerticalAlignment="Center">bar * a</TextBlock>

                            <Label Grid.Column="0" Grid.Row="2">Modules</Label>
                            <TextBox Grid.Column="1" Grid.Row="2" Style="{StaticResource Colored}"/>
                            <TextBox Grid.Column="3" Grid.Row="2" Style="{StaticResource Colored}"/>

                            <Label Grid.Column="0" Grid.Row="3">Inlet Nozzles</Label>
                            <TextBox Grid.Column="1" Grid.Row="3" Style="{StaticResource Colored}"/>
                            <TextBox Grid.Column="2" Grid.Row="3" Style="{StaticResource Colored}"/>
                            <TextBox Grid.Column="3" Grid.Row="3" Style="{StaticResource Colored}"/>

                            <Label Grid.Column="0" Grid.Row="4">Outlet Nozzles</Label>
                            <TextBox Grid.Column="1" Grid.Row="4" Style="{StaticResource Colored}"/>
                            <TextBox Grid.Column="2" Grid.Row="4" Style="{StaticResource Colored}"/>
                            <TextBox Grid.Column="3" Grid.Row="4" Style="{StaticResource Colored}"/>

                            <Label Grid.Column="0" Grid.Row="5">Bends</Label>
                            <TextBox Grid.Column="1" Grid.Row="5" Style="{StaticResource Colored}"/>
                            <TextBox Grid.Column="3" Grid.Row="5" Style="{StaticResource Colored}"/>

                            <Label Grid.Column="0" Grid.Row="6" FontWeight="Bold">Total</Label>
                            <TextBox Grid.Column="3" Grid.Row="6" Style="{StaticResource Colored}"/>

                            <Label Grid.Column="0" Grid.Row="8" FontWeight="Bold">Shell Side</Label>

                            <Label Grid.Column="1" Grid.Row="8" HorizontalContentAlignment="Center" VerticalContentAlignment="Center">V</Label>
                            <Label Grid.Column="1" Grid.Row="8" HorizontalContentAlignment="Right" VerticalContentAlignment="Center">m/s</Label>

                            <TextBlock Grid.Column="2" Grid.Row="8" HorizontalAlignment="Center">P*V<Run BaselineAlignment="Superscript">2</Run></TextBlock>
                            <TextBlock Grid.Column="2" Grid.Row="8" HorizontalAlignment="Right" >kg/m*s<Run BaselineAlignment="Superscript">2</Run></TextBlock>

                            <TextBlock Grid.Column="3" Grid.Row="8" HorizontalAlignment="Center" VerticalAlignment="Center">ΔP</TextBlock>
                            <TextBlock Grid.Column="3" Grid.Row="8" HorizontalAlignment="Right" VerticalAlignment="Center">bar * a</TextBlock>

                            <Label Grid.Column="0" Grid.Row="9">Modules</Label>
                            <TextBox Grid.Column="1" Grid.Row="9" Style="{StaticResource Colored}"/>
                            <TextBox Grid.Column="3" Grid.Row="9" Style="{StaticResource Colored}"/>

                            <Label Grid.Column="0" Grid.Row="10">Inlet Nozzles</Label>
                            <TextBox Grid.Column="1" Grid.Row="10" Style="{StaticResource Colored}"/>
                            <TextBox Grid.Column="2" Grid.Row="10" Style="{StaticResource Colored}"/>
                            <TextBox Grid.Column="3" Grid.Row="10" Style="{StaticResource Colored}"/>

                            <Label Grid.Column="0" Grid.Row="11">Outlet Nozzles</Label>
                            <TextBox Grid.Column="1" Grid.Row="11" Style="{StaticResource Colored}"/>
                            <TextBox Grid.Column="2" Grid.Row="11" Style="{StaticResource Colored}"/>
                            <TextBox Grid.Column="3" Grid.Row="11" Style="{StaticResource Colored}"/>

                            <Label Grid.Column="0" Grid.Row="12">Pressure drop in all central baffle spaces (ΔPc)</Label>
                            <TextBox Grid.Column="3" Grid.Row="12" Style="{StaticResource Colored}"/>

                            <Label Grid.Column="0" Grid.Row="13">Pressure drop in all baffle windows (ΔPw)</Label>
                            <TextBox Grid.Column="3" Grid.Row="13" Style="{StaticResource Colored}"/>

                            <Label Grid.Column="0" Grid.Row="14">Pressure drop in the extrance and exit baffle spaces (ΔPe)</Label>
                            <TextBox Grid.Column="3" Grid.Row="14" Style="{StaticResource Colored}"/>

                            <Label Grid.Column="0" Grid.Row="15" FontWeight="Bold">Total</Label>
                            <TextBox Grid.Column="3" Grid.Row="15" Style="{StaticResource Colored}"/>
                        </Grid>
                    </StackPanel>
                    <StackPanel Orientation="Horizontal">
                        <Grid Width="800" Height="400" VerticalAlignment="Top">
                            <Grid.ColumnDefinitions>
                                <ColumnDefinition Width="198*"/>
                                <ColumnDefinition Width="120*"/>
                                <ColumnDefinition Width="120*"/>
                                <ColumnDefinition Width="30*"/>
                                <ColumnDefinition Width="120*"/>
                                <ColumnDefinition Width="120*"/>
                            </Grid.ColumnDefinitions>
                            <Grid.RowDefinitions>
                                <RowDefinition/>
                                <RowDefinition/>
                                <RowDefinition/>
                                <RowDefinition/>
                                <RowDefinition/>
                                <RowDefinition/>
                                <RowDefinition/>
                                <RowDefinition/>
                                <RowDefinition/>
                                <RowDefinition/>
                                <RowDefinition/>
                            </Grid.RowDefinitions>

                            <Label FontWeight="Bold" Grid.Column="0" Grid.Row="0">Heat Transfer Data</Label>
                            <Label Grid.Column="0" Grid.Row="1">Wall temperature</Label>
                            <Label Grid.Column="0" Grid.Row="2">Wall Consistency Index</Label>
                            <Label Grid.Column="0" Grid.Row="3">Nusselt</Label>
                            <Label Grid.Column="0" Grid.Row="4">K Side</Label>
                            <Label Grid.Column="0" Grid.Row="5">Fouling Factor</Label>

                            <TextBox Style="{StaticResource Colored}" Grid.Column="1" Grid.Row="1"/>
                            <TextBox Style="{StaticResource Colored}" Grid.Column="2" Grid.Row="1"/>
                            <TextBox Style="{StaticResource Colored}" Grid.Column="1" Grid.Row="2"/>
                            <TextBox Style="{StaticResource Colored}" Grid.Column="2" Grid.Row="2"/>
                            <TextBox Style="{StaticResource Colored}" Grid.Column="1" Grid.Row="3"/>
                            <TextBox Style="{StaticResource Colored}" Grid.Column="2" Grid.Row="3"/>
                            <custom1:CustonCheck Grid.Column="1" Grid.Row="4"/>
                            <custom1:CustonCheck Grid.Column="2" Grid.Row="4"/>
                            <TextBox Style="{StaticResource Default}" Grid.Column="1" Grid.Row="5" Grid.ColumnSpan="2"/>

                            <TextBox Style="{StaticResource Colored}" Grid.Column="4" Grid.Row="1"/>
                            <TextBox Style="{StaticResource Colored}" Grid.Column="5" Grid.Row="1"/>
                            <TextBox Style="{StaticResource Colored}" Grid.Column="4" Grid.Row="2"/>
                            <TextBox Style="{StaticResource Colored}" Grid.Column="5" Grid.Row="2"/>
                            <TextBox Style="{StaticResource Colored}" Grid.Column="4" Grid.Row="3"/>
                            <TextBox Style="{StaticResource Colored}" Grid.Column="5" Grid.Row="3"/>
                            <custom1:CustonCheck Grid.Column="4" Grid.Row="4"/>
                            <custom1:CustonCheck Grid.Column="5" Grid.Row="4"/>
                            <TextBox Style="{StaticResource Default}" Grid.Column="4" Grid.Row="5" Grid.ColumnSpan="2"/>

                            <Label Grid.Column="2" Grid.Row="6" HorizontalAlignment="Center" VerticalAlignment="Center">Inlet</Label>
                            <Label Grid.Column="4" Grid.Row="6" HorizontalAlignment="Center" VerticalAlignment="Center">Outlet</Label>

                            <Label Grid.Row="7" Grid.Column="0">K Unfouled</Label>
                            <Label Grid.Row="8" Grid.Column="0">K Fouled</Label>
                            <Label Grid.Row="9" Grid.Column="0">K Global Fouled</Label>
                            <Label Grid.Row="10" Grid.Column="0">K Effective</Label>


                            <TextBox Style="{StaticResource Colored}" Grid.Row="7" Grid.Column="2"/>
                            <TextBox Style="{StaticResource Colored}" Grid.Row="7" Grid.Column="4"/>
                            <custom1:CustonCheck Grid.Column="2" Grid.Row="8"/>
                            <custom1:CustonCheck Grid.Column="4" Grid.Row="8"/>
                            <custom1:CustonCheck Grid.Column="2" Grid.Row="9" Grid.ColumnSpan="3"/>
                            <TextBox Style="{StaticResource Colored}" Grid.Row="10" Grid.Column="2" Grid.ColumnSpan="3"/>
                        </Grid>
                        <Grid Width="800" VerticalAlignment="Top">
                            <Grid.ColumnDefinitions>
                                <ColumnDefinition/>
                                <ColumnDefinition Width="120"/>
                                <ColumnDefinition Width="120"/>
                                <ColumnDefinition Width="120"/>
                            </Grid.ColumnDefinitions>
                            <Grid.RowDefinitions>
                                <RowDefinition/>
                                <RowDefinition/>
                                <RowDefinition/>
                                <RowDefinition/>
                                <RowDefinition/>
                                <RowDefinition/>
                                <RowDefinition/>
                                <RowDefinition/>
                                <RowDefinition/>
                                <RowDefinition/>
                                <RowDefinition/>
                                <RowDefinition/>
                                <RowDefinition/>
                                <RowDefinition/>
                                <RowDefinition/>
                            </Grid.RowDefinitions>
                            <Label Grid.Column="0" Grid.Row="0" FontWeight="Bold">Vibrations</Label>
                            <Label Grid.Column="1" Grid.Row="0" FontWeight="Bold">Inlet</Label>
